<h1>Entries#index</h1>
<p>Find me in app/views/entries/index.html.erb</p>


 <%= simple_form_for [@milestone, @entry] do |f| %>
     <%= f.association :name, label: 'Milestone', class: 'form-control-sm' %>
     <%= f.input :goal, label: 'Entry' %>
     <%= f.submit %>
    <% end %>

<% @entries.each do |entry| %>
<p>entry title: <%= entry.goal %></p>
<p> entry milestone: <%= entry.milestones.first.title %></p>
<p>entry milestone: <%= entry.milestones.last.title %></p>
<% end %>
