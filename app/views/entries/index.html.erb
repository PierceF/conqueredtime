<h1>Entries#index</h1>
<p>Find me in app/views/entries/index.html.erb</p>

<form action="/entries" method="post">
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <label for="goals">Create a category: </label>
  <input type="text" name="entry[goal]" id="goal">

  <input type="submit">
</form>

<% @entries.each do |entry| %>
<p>entry title: <%= entry.goal %></p>
<p> entry milestone: <%= entry.milestones.first.title %></p>
<p>entry milestone: <%= entry.milestones.last.title %></p>
<% end %>
